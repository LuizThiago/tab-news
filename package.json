{
  "name": "tab-news",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/LuizThiago/tab-news#readme",
  "bugs": {
    "url": "https://github.com/LuizThiago/tab-news/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuizThiago/tab-news.git"
  },
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker-compose -f infra/compose.yaml up -d",
    "services:stop": "docker-compose -f infra/compose.yaml stop",
    "services:down": "docker-compose -f infra/compose.yaml down",
    "build": "next build",
    "start": "next start",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate up",
    "migration:down": "node-pg-migrate down"
  },
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.6.2"
  }
}
